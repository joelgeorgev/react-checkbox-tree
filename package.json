{
  "name": "react-checkbox-tree",
  "version": "1.0.0",
  "description": "A react app showcasing a simple checkbox tree component.",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.4",
    "coveralls": "^3.1.0",
    "gh-pages": "^2.2.0",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --check \"./**/*.{js,json,md}\"",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ]
  },
  "repository": "https://github.com/joelgeorgev/react-checkbox-tree",
  "keywords": [
    "React",
    "reactjs",
    "checkbox",
    "tree",
    "normalize"
  ],
  "author": "Joel George V",
  "homepage": "https://joelgeorgev.github.io/react-checkbox-tree",
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
