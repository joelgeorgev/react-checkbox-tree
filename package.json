{
  "name": "react-checkbox-tree",
  "version": "1.0.0",
  "description": "A react app showcasing a simple checkbox tree component.",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "coveralls": "^3.1.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.0.5",
    "react-scripts": "^4.0.3",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "compile": "tsc",
    "compile:watch": "tsc --watch",
    "format": "prettier --check \"./**/*.{ts,tsx,json,md}\"",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx"
    ]
  },
  "repository": "https://github.com/joelgeorgev/react-checkbox-tree",
  "keywords": [
    "React",
    "reactjs",
    "checkbox",
    "tree",
    "normalize"
  ],
  "author": "Joel George V",
  "homepage": "https://joelgeorgev.github.io/react-checkbox-tree",
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
